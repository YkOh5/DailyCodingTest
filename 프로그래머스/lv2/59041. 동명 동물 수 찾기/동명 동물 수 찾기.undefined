-- 코드를 입력하세요
SELECT NAME, COUNT(*) AS COUNT
FROM ANIMAL_INS
WHERE IFNULL(NAME, "") <> ""
GROUP BY NAME
HAVING COUNT >= 2
ORDER BY NAME ASC;


